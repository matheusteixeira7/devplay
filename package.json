{
  "name": "devplay",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "content-management:db:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate database/content/typeorm/migration/migration  -d ./database/content/typeorm/migration.datasource.ts",
    "content-management:db:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./database/content/typeorm/migration.datasource.ts",
    "content-management:db:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./database/content/typeorm/migration.datasource.ts",
    "content-streaming:db:setup": "ts-node ./database/content/dynamodb/dynamodb.setup.ts",
    "billing:db:generate": "drizzle-kit generate --config database/billing/drizzle/drizzle-migration.config.ts",
    "billing:db:migrate": "drizzle-kit migrate --config database/billing/drizzle/drizzle-migration.config.ts",
    "billing:db:push": "drizzle-kit push --config database/billing/drizzle/drizzle-migration.config.ts",
    "billing:db:drop": "drizzle-kit drop --config database/billing/drizzle/drizzle-migration.config.ts",
    "db:setup": "pnpm run content-management:db:migrate && pnpm run identity:db:migrate && pnpm run content-streaming:db:setup && pnpm run billing:db:migrate",
    "docker:stop": "docker-compose stop",
    "docker:up": "docker-compose up --detach",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' './*.{js,json}'",
    "format:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts' './*.{js,json}'",
    "identity:db:generate": "prisma migrate dev",
    "identity:db:migrate": "prisma db push --accept-data-loss",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "lint:check": "eslint '{src,apps,libs,test}/**/*.ts'",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "npm run test:type && npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:db:migrate": "dotenv -e .env.test -- prisma db push",
    "test:db:setup": "dotenv -e .env.test -- pnpm run db:setup",
    "test:e2e": "jest --runInBand --config ./test/jest.e2e.ts --forceExit",
    "test:e2e:watch": "pnpm run test:e2e -- --watch",
    "test:integration": "jest --runInBand --config ./test/jest.integration.ts",
    "test:integration:watch": "pnpm run test:integration -- --watch",
    "test:type": "tsc --noEmit",
    "test:unit": "jest --config ./test/jest.unit.ts",
    "test:unit:watch": "pnpm run test:unit -- --watch"
  },
  "engines": {
    "npm": "please-use-npm"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@aws-sdk/client-dynamodb": "^3.511.0",
    "@knaadh/nestjs-drizzle-postgres": "^1.0.0",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/axios": "^3.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/graphql": "^12.0.7",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@prisma/client": "^5.21.1",
    "aws-sdk": "^2.1555.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "fs-extra": "^11.1.1",
    "graphql": "^16.7.1",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.10.0",
    "nock": "14.0.0-beta.4",
    "pg": "^8.11.3",
    "postgres": "^3.4.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0",
    "winston": "^3.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@faker-js/faker": "^9.0.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/jest-when": "^3.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-project-structure": "^3.2.1",
    "factory.ts": "^1.4.2",
    "fishery": "^2.2.2",
    "globals": "^15.10.0",
    "jest": "^29.5.0",
    "jest-when": "^3.6.0",
    "knex": "^3.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prisma": "^5.21.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "schema": "database/identity/prisma/schema.prisma"
  }
}
